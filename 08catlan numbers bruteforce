LL catlan_numbers(int n)
{
    if(n==0) return 1;
    LL temp=0;
    for(int i=1;i<=n;i++)
    {
        temp+=catlan_numbers(i-1)*catlan_numbers(n-i);
    }
    return temp;
}
int main()
{
    //freopen("input.txt","r",stdin);
    cout<<catlan_numbers(3);
    return 0;
}
